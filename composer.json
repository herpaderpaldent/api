{
  "name": "eveseat/api",
  "description": "SeAT API Interface",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Leon Jacobs",
      "email": "leonja511@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seat\\Api\\": "src/"
    }
  },
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "repositories" : [
    {
      "type": "vcs",
      "url": "https://github.com/herpaderpaldent/eveapi"
    },
    {
      "type": "vcs",
      "url": "https://github.com/herpaderpaldent/services"
    },
    {
      "type": "vcs",
      "url": "https://github.com/herpaderpaldent/web"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "5.5.*",
    "eveseat/eveapi": "dev-master",
    "eveseat/services": "dev-master",
    "eveseat/web": "dev-master",
    "darkaonline/l5-swagger": "5.5.*",
    "zircote/swagger-php": "2.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seat\\Api\\ApiServiceProvider"
      ]
    }
  }
}
